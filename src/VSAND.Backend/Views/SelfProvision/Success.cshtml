@model VSAND.Data.ViewModels.SelfProvision.SelfProvisionForm

@{
    Layout = "_Layout.cshtml";
    string siteUrl = ViewData["SiteUrl"].ToString();
}

<div class="container py-4 py-lg-5 my-lg-5 px-4 px-sm-0">
    <div class="row">
        <div class="col-xl-12">
            <h2 class="fs-xxl fw-500 mt-4 text-white text-center">
                Account Successfully Created!
            </h2>
        </div>
        <div class="col-xl-6 ml-auto mr-auto">
            <div class="card p-4 rounded-plus bg-faded">
                <p>An account has been created with the email address: @(Model.Email).</p>
                <a class="btn btn-primary" href="/Account/Logout">Click Here to Login</a>
            </div>
        </div>
    </div>
</div>
