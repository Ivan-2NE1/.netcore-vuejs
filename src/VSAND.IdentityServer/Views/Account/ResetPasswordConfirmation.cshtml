@{
    ViewData["Title"] = "Reset password confirmation";
    var returnUrl = ViewData.ContainsKey("ReturnUrl") ? ViewData["ReturnUrl"] : "";
}

<h2>@ViewData["Title"]</h2>
<p>
    Your password has been reset. Please <a asp-action="Login" asp-route-returnUrl="@returnUrl">click here to log in</a>.
</p>